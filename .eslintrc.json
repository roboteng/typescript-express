{
    "env": {
        "browser": true,
        "es2021": true
    },
    "ignorePatterns" :[
      "build/",
      "node_modules/"
    ],
    "extends": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars":"error"
    }
}
